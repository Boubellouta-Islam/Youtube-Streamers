{
    "name" : "Youtube Streamers",
    "description" : "Watch All Favorite Youtube Streamers At The Same Time",
    "version" : "1.0",
    "manifest_version" : 3,
    "icons": {
        "16": "/images/YtbStr-icon-16.png",
        "48": "/images/YtbStr-icon-64.png",
        "128": "/images/YtbStr-icon-128.png"
    },
    "action":{
        "default_icon": {
            "16": "/images/YtbStr-icon2-16.png",
            "48": "/images/YtbStr-icon2-64.png",
            "128": "/images/YtbStr-icon2-128.png"
        },
        "default_popup": "window.html"
    },
    "permissions" : ["tabs", "system.display"]
}